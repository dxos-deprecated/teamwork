{
  "parser": "@typescript-eslint/parser",
  "extends": [
    "semistandard"
  ],
  "plugins": [
    "@typescript-eslint/eslint-plugin"
  ],
  "rules": {
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": ["error"],
    "indent": "off",
    "@typescript-eslint/indent": ["error", 2]
  }
}
