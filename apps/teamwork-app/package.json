  "scripts": {
    "analyzer": "webpack --config webpack-analyzer.config.js",
    "build": "webpack -p",
    "debug": "DOT_ENV=.env.debug webpack-dev-server --mode development",
    "debug:localhost": "CONFIG=localhost.yml webpack-dev-server --mode development",
    "dist": "rm -rf dist && NODE_ENV=production webpack",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "playwright": "./scripts/start-and-wait.sh && yarn test:playwright",
    "start": "VERBOSE=true NODE_ENV=development webpack-dev-server",
    "start:public": "VERBOSE=true PUBLIC_URL=/teamwork npm run start",
    "test": "jest --rootDir ./src --verbose --passWithNoTests",
    "test:playwright": "jest --bail --verbose --forceExit --rootDir ./tests/playwright"
  },
