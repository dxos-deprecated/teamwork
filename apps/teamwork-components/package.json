{
  "name": "teamwork-components",
  "version": "1.0.0",
  "main": "dist/es/index.js",
  "module": "dist/es/index.js",
  "scripts": {
    "build": "npm run clean && babel ./src --out-dir ./dist/es --ignore \"**/*.test.js\"",
    "build:storybook": "build-storybook -c .storybook -o ./out --quiet",
