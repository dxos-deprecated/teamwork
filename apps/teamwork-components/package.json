    "debug": "^4.1.1",
    "faker": "^4.1.0",
    "immutability-helper": "^3.0.2",
    "lodash.defaultsdeep": "^4.6.1",
    "lodash.get": "^4.4.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hotkeys": "^2.0.0",
    "react-resize-aware": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/preset-react": "^7.9.4",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "babel-jest": "^25.5.1",
